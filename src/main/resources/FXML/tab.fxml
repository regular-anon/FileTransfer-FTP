<Tab text="Unknown Connection" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <HBox alignment="BASELINE_CENTER" prefHeight="35.0" prefWidth="908.0">
                    <children>
                        <Button mnemonicParsing="false" text="Disconnect" id="disconnectButton">
                            <HBox.margin>
                                <Insets right="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Refresh">
                            <HBox.margin>
                                <Insets left="10.0" right="15.0" />
                            </HBox.margin>
                        </Button>
                        <TextField id="searchQuery" />
                        <Button id="searchButton" mnemonicParsing="false" text="Search" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </VBox.margin>
                </HBox>
                <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="1000.0" prefWidth="790.0">
                    <children>
                        <TableView maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="1000.0" prefWidth="790.0" id="tableView">
                            <columns>
                                <TableColumn maxWidth="371.0" minWidth="371.0" prefWidth="371.0" text="File name" id="fileNameColumn"/>
                                <TableColumn maxWidth="212.0" minWidth="212.0" prefWidth="212.0" text="Size" id="sizeColumn"/>
                                <TableColumn maxWidth="-Infinity" minWidth="212.0" prefWidth="212.0" text="Last Modified" id="lastModifiedColumn"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                        <ImageView fitHeight="117.0" fitWidth="117.0" id="imgView">
                            <image>
                                <Image url="@../Photos/fileDrop.png" />
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
                <HBox alignment="TOP_CENTER" prefHeight="23.0" prefWidth="946.0" spacing="150.0">
                    <children>
                        <Button id="uploadButton" mnemonicParsing="false" text="Upload" />
                        <Button id="downloadButton" mnemonicParsing="false" text="Download" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </content>
